<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<context:component-scan base-package="com.samsung.biz"/>
	
	<bean id="userVO" class="com.samsung.biz.user.vo.UserVO"/>
	<bean id="userDAO" class="com.samsung.biz.user.impl.UserDAO"/>
	<bean id="BoardVO" class="com.samsung.biz.board.vo.BoardVO"/>
	<bean id="BoardDAO" class="com.samsung.biz.board.impl.BoardDAO"/>
	
	
</beans>
